{
    "manifest_version": 3,
    "name": "AI4Chat Login Checker",
    "version": "1.0",
    "permissions": [
      "cookies",
      "storage",
      "scripting",
      "tabs"
    ],
    "host_permissions": [
      "https://app.ai4chat.co/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  
  
  
  